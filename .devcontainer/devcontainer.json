    "settings": {
      "workbench.iconTheme": "material-icon-theme",
      "workbench.colorCustomizations": {
        "[GitHub Dark Dimmed][Railgun]": {
          "editor.background": "#161616",
          "editorInlayHint.typeBackground": "#ff000000",
          "editorInlayHint.typeForeground": "#5a5a5a"
        },
        "[MoonGun Dark Inesh][Maron Themes: Nordic Warriors][Vasuki Dark Theme]": {
          "editorInlayHint.typeBackground": "#ff000000",
          "editorInlayHint.parameterBackground": "#ff000000",
          "editorInlayHint.background": "#ff000000",
          "editorInlayHint.typeForeground": "#787878",
          "editorInlayHint.parameterForeground": "#787878",
          "editorInlayHint.foreground": "#787878"
        },
        "[Github Light Theme - Gray][FlatUI][Fleet Dark][Moegi Light Vitesse]": {
          "editorInlayHint.typeBackground": "#ff000000",
          "editorInlayHint.typeForeground": "#a0a0a0"
        }
      },
      "css.lint.unknownAtRules": "ignore",
      "scss.lint.unknownAtRules": "ignore",
      "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
      "editor.inlayHints.enabled": "off",
      "editor.cursorBlinking": "smooth",
      "editor.cursorStyle": "block",
      "editor.cursorSmoothCaretAnimation": "on",
      "window.nativeTabs": true,
      "codeium.aggressiveShutdown": true,
      "eslint.validate": ["typescript", "javascript", "vue"],
      "files.autoSave": "off",
      "editor.formatOnPaste": true,
      "editor.formatOnSave": true,
      "prettier.singleQuote": true,
      "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "workbench.sideBar.location": "right",
      "git.enableSmartCommit": true,
      "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "git.confirmSync": false,
      "git.autofetch": true,
      "workbench.editor.editorActionsLocation": "titleBar",
      "accessibility.verbosity.keybindingsEditor": false,
      "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "security.workspace.trust.untrustedFiles": "open",
      "firedbg.showWelcome": false,
      "svelte.enable-ts-plugin": true,
      "git.openRepositoryInParentFolders": "always",
      "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "rust-analyzer.inlayHints.typeHints.enable": true,
      "javascript.inlayHints.parameterNames.enabled": "literals",
      "javascript.inlayHints.parameterTypes.enabled": true,
      "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
      "javascript.inlayHints.variableTypes.enabled": true,
      "typescript.inlayHints.enumMemberValues.enabled": true,
      "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
      "typescript.inlayHints.parameterNames.enabled": "literals",
      "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
      "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "explorer.confirmDelete": false,
      "tabnine.experimentalAutoImports": true,
      "codeium.enableSearch": true,
      "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "errorLens.enabled": true,
      "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "symbols.hidesExplorerArrows": false,
